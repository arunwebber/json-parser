{
  "manifest_version": 3,
  "name": "JSON Formatter",
  "version": "1.1.1",
  "description": "Open a separate window to parse and format JSON.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open JSON Formatter",
    "default_icon": {
      "16": "images/icon_16.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    }
  },
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "homepage_url": "https://hashpallabs.com/",
  "author": "HashPalLabs"
}
